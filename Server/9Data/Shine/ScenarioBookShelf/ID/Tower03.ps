; Signal Waiting

open [WaitClearStage]

	if CurrentStage < 3
    then open
    	interruptset Sec "Tick" 1 10 "onTick0".
    close
	    else open
		if CurrentStage < 6
		then open
			interruptset Sec "Tick" 1 10 "onTick".
	    close
	    else open
			interruptset Sec "Tick" 1 10 "onTick2".
	    close
    close
	
    pause Sec 5.

    infinite
    open
        waitinterrupt InterruptBlock "InterruptArg".
    call InterruptBlock.
    close
close

open [WaitKillBoss]
    interruptset DeadIndex "BossDead" 1 TargetObject "onBossDead".

    pause Sec 5.

    infinite
    open
        waitinterrupt InterruptBlock "InterruptArg".
    call InterruptBlock.
    close
close

open [WaitOpenPrison]
    interruptset NPCClickHandle "" 1 DoorHandle "onOpenPrison".

    infinite
    open
        waitinterrupt InterruptBlock "InterruptArg".
    call InterruptBlock.
    close
close


; EventHandler

open [onEntranceClick]
    var Clicker 0.
    whoclickme Clicker InterruptArg.
    linkto Clicker "Croad01" "Croad01" 2856 7439.
    interruptset NPCClickHandle "EntranceClick" 1 Entrance "onEntranceClick".
close

open [onExitClick]
    var Clicker 0.
    whoclickme Clicker InterruptArg.
    linkto Clicker "Croad01" "Croad01" 2856 7439.
    interruptset NPCClickHandle "ExitClick" 1 Exit "onExitClick".
close

open [onBossDead]
    suicide all.
    break "WaitKillBoss".
close

open [onOpenPrison]
	var Slot         ""
    	OpenerHandle "".

    whoclickme OpenerHandle InterruptArg.
    invensearch Slot OpenerHandle Key.

    if Slot =!= ""
    then open
        itemerase OpenerHandle Key 1.
        break "WaitOpenPrison".
    close
    else open
        interruptset NPCClickHandle "" 1 DoorHandle "onOpenPrison".
    close
close

open [onTick0]
    var Total 0
        Except 0
        Have 0.

    Total = @Count("all").
    Except = @Count("Lab_Slime").

    if Except > 0
    then open
        HasAbstate Have TimeAtkHandle "StaTimeAttackMini" 1.

        if Have == 1
        then open
            Total = Total - Except.
        close
        else open
            if Warn == 1
            then open
                HasAbstate Have TimeAtkHandle "StaTimeAttackMini" 2.
            close
        close
        
    	if Have == 0
	    then open
            if Warn == 1
            then open
            	BroadcastCode all "ERR_NOERR_TIMEATK_EXPLODE".
                Warn = 0.
                MissCount = MissCount + 1.
                interrupterase "AwakeWarning".
                interrupterase "AwakeWarning2".
            close
	    close
    close
    
    if Total == 0
    then open
    	Warn = 1.
        break "WaitClearStage".
    close
    else open
    	if Except > 0
        then open
        	interruptset Sec "Tick" 1 1 "onTick0".
        close
        else open
        	interruptset Sec "Tick" 1 10 "onTick0".
        close
    close
close


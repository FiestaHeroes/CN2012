open [KeyDrop]
    if KeyDropRate > @Random(0 99) then
    open
        itemdrop InterruptArg KeyDropIndex 1000000.     ; 열쇠드롭(100% 확률)
        broadcast all "ItemDrop".                       ; 열쇠가 떨어졌음을 모두에게 알려줌
        interrupterase "KeyDrop".                       ; 모든 KeyDrop 인터럽트를 없앰
    close
    else
    open
        KeyDropRate = KeyDropRate + 5.
    close
close

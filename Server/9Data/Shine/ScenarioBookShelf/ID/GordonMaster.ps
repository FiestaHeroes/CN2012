open [QuestSuccess]
    var GordonX     0
        GordonY     0
        GordonPos   0.

    GordonPos = @WhereIs(GordonMaster).
    GordonX = @RemoveFirst("GordonPos" " ").
    GordonY = @RemoveFirst("GordonPos" " ").

    questresult Suc.                   ; 성공패킷 날림
	
	interruptclear.
    
	npcstand	ReturnGate 		"Gate_ID_Exit" 		1893	245		0 1000	"Normal".
	npcstand	CompleteGate 	"Gate_ID_Complete" 	GordonX	GordonY	0 1000	"Normal".
	
	SendMapInMobInfo ReturnGate.
	SendMapInMobInfo CompleteGate.
	
	interruptset	NPCClickHandle	"ReturnGateClick"		1	ReturnGate		"onReturnGateClick".
	interruptset	NPCClickHandle	"CompleteGateClick"		1	CompleteGate		"onCompleteGateClick".
	
	infinite
    open
        waitinterrupt InterruptBlock "InterruptArg".
        call InterruptBlock.
    close
close

